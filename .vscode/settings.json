{
  "css.validate": false,
  "tailwindCSS.emmetCompletions": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "editor.inlineSuggest.enabled": true,
  "tailwindCSS.experimental.classRegex": [
    ["class[sName]*\\s*[:=]\\s*['\"`]([^'\"`]*)['\"`]", "([^\\s'\"`]+)"]
  ]
}
